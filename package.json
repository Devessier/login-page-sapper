        "postcss-cli": "^7.1.0",
        "prettier": "^2.0.4",
        "sapper": "^0.27.12",
        "svelte": "^3.20.1",
        "svelte-loader": "^3.1.4",
        "svelte-preprocess-sass": "^0.2.0",
        "tailwindcss": "^1.2.0",
        "webpack": "^5.75.0"
    }
