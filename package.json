    "version": "0.0.1",
    "scripts": {
        "dev": "sapper dev",
        "start": "node __sapper__/build",
        "cy:run": "cypress run",
        "cy:open": "cypress open",
        "test": "run-p --race dev cy:run",
        "dev:tailwindcss": "postcss static/tailwind.css -o static/main.css -w",
